  "dependencies": {
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "log4js": "^1.1.1",
    "md5": "^2.2.1",
    "multer": "^1.3.0",
    "should": "^10.0.0"
  },
  "main": "app.js",
  "scripts": {
